
.root_tab1 {
	display: flex;
	flex-direction: column;
}
.container {
	padding: 0 0;
}
.input {
	height: 45px;
	padding-left: 20px;
	padding-right: 20px;
	background: #234425;
	border-radius: 50px;
	border: 0px solid #868686;
	margin-left: 20px;
	margin-right: 20px;
}
.content1 {
	padding: 40rpx 20rpx 0 0rpx;
}
.content1_categories {
	display: grid;
	grid-template-columns: repeat(4, 25%);
	grid-template-rows: 160rpx;
	padding: 10rpx 0rpx 0 0rpx;
}

